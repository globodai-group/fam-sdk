{
  "name": "@freelance-and-me/sdk",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for the Freelance-and-Me API",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./webhooks": {
      "types": "./dist/webhooks/index.d.ts",
      "import": "./dist/webhooks/index.mjs",
      "require": "./dist/webhooks/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc",
    "knip": "knip",
    "madge": "madge --circular --extensions ts src/",
    "check": "pnpm run typecheck && pnpm run lint && pnpm run format:check",
    "ci:check": "pnpm run check && pnpm run knip && pnpm run madge",
    "ci:test": "pnpm run test:coverage",
    "ci:build": "pnpm run build",
    "prepublishOnly": "pnpm run ci:check && pnpm run ci:test && pnpm run ci:build",
    "prepare": "husky"
  },
  "keywords": [
    "freelance-and-me",
    "mangopay",
    "sdk",
    "api",
    "typescript",
    "payments"
  ],
  "author": "Freelance-and-Me",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/globodai-group/freelance-and-me-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/globodai-group/freelance-and-me-sdk/issues"
  },
  "homepage": "https://github.com/globodai-group/freelance-and-me-sdk#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "bun@1.3.5",
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.17.0",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "husky": "^9.1.7",
    "knip": "^5.41.1",
    "lint-staged": "^16.2.7",
    "madge": "^8.0.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typedoc": "^0.28.15",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vitest": "^4.0.16"
  }
}
